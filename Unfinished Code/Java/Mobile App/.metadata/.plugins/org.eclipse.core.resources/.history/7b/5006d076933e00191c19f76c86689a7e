package arrays;

public class array {
	char[ ][ ] my2DArray = {{'a','b','c','d'}, {'e','f','g','h'}}; {

	for (int i=0; i<my2DArray.length; i++)
	{
	    for (int j=0; j<my2DArray[i].length; j++)
	    {
	       if(my2DArray[i][j] == 'e')
	       {
	            System.out.println("row: " + i);
	            System.out.println( "col: " + j);

	       }
	    }
	}
}}
